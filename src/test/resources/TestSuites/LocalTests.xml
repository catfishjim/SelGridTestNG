<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="LocalTests" configfailurepolicy="continue" parallel="classes" thread-count="1">
	<parameter name="environment" value="local" />
	<listeners>
		<listener class-name="listeners.AnnotationTransformer" />
		<listener class-name="listeners.TestListener" />
	</listeners>
	<test name="LandingPageTests">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.practicetestautomation.tests.DemoPageTests" />
			<!-- 	<class name="com.practicetestautomation.tests.DemoPageTests"/> -->
		</classes>
	</test>

	<test name="DemoPageTests">
		<parameter name="browser" value="chrome" />
		<classes>
			<class name="com.practicetestautomation.tests.DemoPageTests" />
	 <!--   <class name="com.practicetestautomation.tests.LandingPageTests"/> -->
		</classes>
	</test>


</suite>
